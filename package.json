{
  "activationEvents": [
    "onCommand:lintel.start"
  ],
  "author": "Mark Florence",
  "badges": [
    {
      "description": "Jest Coverage",
      "href": "https://github.com/mflorence99/lintel/issues",
      "url": "https://img.shields.io/badge/coverage-97.66%25-green"
    },
    {
      "description": "License: MIT",
      "href": "https://opensource.org/licenses/MIT",
      "url": "https://img.shields.io/badge/License-MIT-yellow.svg"
    },
    {
      "description": "CircleCI",
      "href": "https://circleci.com/gh/mflorence99/lintel",
      "url": "https://circleci.com/gh/mflorence99/lintel.svg?style=shield"
    }
  ],
  "bugs": {
    "email": "mflo999@gmail.com",
    "url": "https://github.com/mflorence99/lintel/issues"
  },
  "categories": [
    "Linters"
  ],
  "contributes": {
    "commands": [
      {
        "category": "Lintel",
        "command": "lintel.start",
        "title": "ESLint Configuration File Visualizer"
      }
    ],
    "configuration": {
      "properties": {
        "lintel.ignoredDirectories": {
          "default": [
            "node_modules",
            "build",
            "dist",
            "out"
          ],
          "description": "List of directories not searched for eslintrc files.",
          "type": "array"
        },
        "lintel.updateDebounceTime": {
          "default": 2500,
          "description": "Debounce time as ESLint configuration files are updated.",
          "minimum": 1,
          "type": "number"
        }
      },
      "title": "Lintel"
    }
  },
  "dependencies": {
    "eslint": "^6"
  },
  "description": "VSCode extension to visualize ESLint configurations",
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular-eslint/builder": "0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
    "@angular-eslint/schematics": "0.0.1-alpha.32",
    "@angular-eslint/template-parser": "0.0.1-alpha.32",
    "@angular/cdk": "^10.0.0",
    "@angular/cli": "~10.0.0",
    "@angular/common": "~10.0.0",
    "@angular/compiler": "~10.0.0",
    "@angular/compiler-cli": "~10.0.0",
    "@angular/core": "~10.0.0",
    "@angular/forms": "~10.0.0",
    "@angular/language-service": "~10.0.0",
    "@angular/platform-browser": "~10.0.0",
    "@angular/platform-browser-dynamic": "~10.0.0",
    "@fortawesome/angular-fontawesome": "^0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@ngxs-labs/data": "^3.0.6",
    "@ngxs/logger-plugin": "^3.6.2",
    "@ngxs/store": "^3.6.2",
    "@ts-stack/markdown": "^1.0.0",
    "@types/escodegen": "0.0.6",
    "@types/eslint": "^7.2.0",
    "@types/esprima": "^4.0.2",
    "@types/jest": "^26",
    "@types/js-yaml": "^3.12.5",
    "@types/json5": "0.0.30",
    "@types/node": "^14.0.14",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^3",
    "@typescript-eslint/parser": "^3",
    "angular2-uuid": "^1.1.1",
    "eslint-plugin-compat": "^3.7.0",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-react-native": "^3.8.1",
    "eslint-plugin-react-redux": "^3.0.3",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-unicorn": "^19",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^26.1.0",
    "jest-junit": "^11.0.1",
    "jest-preset-angular": "^8.1.3",
    "js-yaml": "^3.14.0",
    "ngx-contextmenu": "^5.4.0",
    "rxjs": "~6.5.4",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.17.1",
    "ts-node": "~8.10.2",
    "tslib": "^2.0.0",
    "typescript": "~3.9.5",
    "zone.js": "~0.10.2"
  },
  "displayName": "Lintel: ESLint Configuration File Visualizer",
  "engines": {
    "vscode": "^1.44.0"
  },
  "galleryBanner": {
    "color": "#424242",
    "theme": "dark"
  },
  "homepage": "https://github.com/mflorence99/lintel/blob/master/README.md",
  "icon": "dist/lintel/assets/lintel.png",
  "keywords": [
    "eslint"
  ],
  "license": "MIT",
  "main": "./dist/ext/lintel.js",
  "name": "lintel",
  "publisher": "mflo999",
  "repository": {
    "type": "git",
    "url": "https://github.com/mflorence99/lintel"
  },
  "scripts": {
    "build": "ng --prod build && tsc -p tsconfig.ext.json",
    "build:ext": "tsc -p tsconfig.ext.json -w",
    "build:src": "ng --watch build",
    "clean:caches": "jest --clearCache",
    "clean:deps": "rm -rf node_modules && rm package-lock.json && npm install",
    "extensions": "ts-node -P tsconfig.bin.json bin/eslint-extensions.ts",
    "lint": "eslint --ignore-pattern '**/*.spec.ts' 'bin/**/*.ts' 'common/**/*.ts' 'ext/**/*.ts' 'src/app/**/*.ts' && stylelint 'src/**/*.css' 'src/**/*.scss'",
    "ng": "ng",
    "package": "npm run build && vsce package",
    "postinstall": "ngcc",
    "publish": "npm run build && vsce publish patch",
    "rules": "ts-node -P tsconfig.bin.json bin/eslint-rules.ts",
    "start": "ng serve",
    "test": "jest --runInBand --coverage && ts-node -P tsconfig.bin.json bin/coverage-icon.ts",
    "test:watch": "jest --watch",
    "test:watch:coverage": "jest --watchAll --coverage"
  },
  "version": "1.0.1"
}
